<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=System.Runtime">
    <Color x:Key="DisabledForegroundColor">#808080</Color>
    <Color x:Key="InputBackgroundColor">#333333</Color>
	<Color x:Key="HighlightBackgroundColor">#4d4d4d</Color>
	<Color x:Key="HighlightBorderColor">#999999</Color>
	<Color x:Key="HighlightForegroundColor">#ffffff</Color>
	<Color x:Key="MainBackgroundColor">#1a1a1a</Color>
	<Color x:Key="MainBorderColor">#000000</Color>
	<Color x:Key="MainForegroundColor">#e6e6e6</Color>

	<Color x:Key="TabBackgroundColor">#2b2933</Color>
	<Color x:Key="ThemeBackgroundColor">#553ba3</Color>
    <Color x:Key="ThemeBorderColor">#3b55a3</Color>
    <Color x:Key="ThemeHighlightColor">#214d5c</Color>
    <Color x:Key="WarningBackgroundColor">#cc2929</Color>
</ResourceDictionary>
