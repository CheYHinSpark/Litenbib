<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:system="clr-namespace:System;assembly=System.Runtime">
	<SolidColorBrush x:Key="MainBackground" Color="#1a1a1a"/>
	<SolidColorBrush x:Key="InputBackground" Color="#333333"/>
	<SolidColorBrush x:Key="HighlightBackground" Color="#4d4d4d"/>
	<SolidColorBrush x:Key="MainForeground" Color="#e6e6e6"/>
	<SolidColorBrush x:Key="DisabledForeground" Color="#999999"/>
	<SolidColorBrush x:Key="HighlightForeground" Color="#ffffff"/>

	<SolidColorBrush x:Key="ThemeBackground" Color="#3b55a3"/>
	<SolidColorBrush x:Key="ThemeBorder" Color="#553ba3"/>
	<SolidColorBrush x:Key="ThemeHighlight" Color="#214d5c"/>
	<SolidColorBrush x:Key="WarningBackground" Color="#cc2929"/>
</ResourceDictionary>
